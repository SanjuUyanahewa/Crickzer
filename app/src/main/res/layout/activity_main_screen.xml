<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/scn21"
    tools:context="com.crickzer.app.crickzer.MainScreen">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="117dp"
        android:layout_marginStart="117dp"
        android:layout_marginTop="33dp"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/scn24" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="76dp"
        android:layout_marginRight="76dp"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/scn25" />

    <ListView
        android:id="@+id/listView1"
        android:layout_width="300dp"
        android:layout_height="170dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView"
        android:layout_marginLeft="29dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="27dp"
        android:scrollbarThumbVertical="@drawable/scn26" />

    <ListView
        android:id="@+id/listView2"
        android:layout_width="220dp"
        android:layout_height="170dp"
        android:layout_alignTop="@+id/listView1"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="21dp"
        android:layout_toEndOf="@+id/listView1"
        android:layout_toRightOf="@+id/listView1"
        android:scrollbarThumbVertical="@drawable/scn27" />


</RelativeLayout>
